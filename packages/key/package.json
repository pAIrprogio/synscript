{
  "name": "@synstack/key",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "description": "Key generation utilities",
  "keywords": [
    "string",
    "typescript",
    "ts",
    "key",
    "key generation"
  ],
  "author": {
    "name": "pAIrprog",
    "url": "https://pairprog.io"
  },
  "homepage": "https://github.com/pAIrprogio/synscript/tree/main/packages/key",
  "repository": {
    "type": "git",
    "url": "https://github.com/pAIrprogio/synscript.git",
    "directory": "packages/key"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test:types": "tsc --noEmit",
    "test:unit": "node --experimental-strip-types --experimental-test-snapshots --no-warnings --test src/**/*.test.ts",
    "test:unit:watch": "node --experimental-strip-types --experimental-test-snapshots --no-warnings --watch --test --watch src/**/*.test.ts",
    "test": "pnpm test:types && pnpm test:unit",
    "prepack": "pnpm build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/key.index.d.ts",
        "default": "./dist/key.index.js"
      },
      "require": {
        "types": "./dist/key.index.d.cts",
        "default": "./dist/key.index.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^22.15.32",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "dist/**/*"
  ]
}
