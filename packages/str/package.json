{
  "name": "@synstack/str",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.4.0",
  "version": "1.0.7",
  "description": "Advanced chainable string manipulation",
  "keywords": [
    "string",
    "typescript",
    "ts",
    "chaining",
    "case"
  ],
  "author": {
    "name": "pAIrprog",
    "url": "https://pairprog.io"
  },
  "homepage": "https://github.com/pAIrprogio/synscript/tree/main/packages/str",
  "repository": {
    "type": "git",
    "url": "https://github.com/pAIrprogio/syn-stack.git",
    "directory": "packages/str"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test:types": "tsc --noEmit",
    "test:unit": "node --import tsx --test src/**/*.test.ts",
    "test:unit:watch": "node --import tsx --watch --test src/**/*.test.ts",
    "test": "yarn test:types && yarn test:unit",
    "prepare": "yarn test && yarn build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/str.index.d.ts",
        "default": "./dist/str.index.js"
      },
      "require": {
        "types": "./dist/str.index.d.cts",
        "default": "./dist/str.index.cjs"
      }
    }
  },
  "dependencies": {
    "@synstack/pipe": "workspace:*",
    "change-case": "^5.4.4"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "files": [
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "dist/**/*"
  ],
  "gitHead": "ed339f5b4ef3a70b7371313234250b1e7445ef69"
}
