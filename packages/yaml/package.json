{
  "name": "@synstack/yaml",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.1.11",
  "description": "Schema-safe and opinionated YAML serialization and deserialization",
  "keywords": [
    "yaml",
    "typescript",
    "ts",
    "serialization",
    "deserialization",
    "zod"
  ],
  "author": {
    "name": "pAIrprog",
    "url": "https://pairprog.io"
  },
  "homepage": "https://github.com/pAIrprogio/synscript/tree/main/packages/yaml",
  "repository": {
    "type": "git",
    "url": "https://github.com/pAIrprogio/synscript.git",
    "directory": "packages/yaml"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test:types": "tsc --noEmit",
    "test:unit": "node --experimental-strip-types --experimental-test-snapshots --no-warnings --test src/**/*.test.ts",
    "test:unit:watch": "node --experimental-strip-types --experimental-test-snapshots --no-warnings --watch --test --watch src/**/*.test.ts",
    "test": "pnpm test:types && pnpm test:unit"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/yaml.index.d.ts",
        "default": "./dist/yaml.index.js"
      },
      "require": {
        "types": "./dist/yaml.index.d.cts",
        "default": "./dist/yaml.index.cjs"
      }
    }
  },
  "dependencies": {
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.32",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.67"
  },
  "peerDependencies": {
    "zod": "^3.25.67 || ^4.0.0"
  },
  "files": [
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "dist/**/*"
  ],
  "gitHead": "ed339f5b4ef3a70b7371313234250b1e7445ef69"
}
