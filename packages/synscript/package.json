{
  "name": "@synstack/synscript",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.4.0",
  "version": "1.0.1",
  "author": {
    "name": "pAIrprog",
    "url": "https://pairprog.io"
  },
  "homepage": "https://github.com/pAIrprogio/synscript/tree/main/packages/synscript",
  "repository": {
    "type": "git",
    "url": "https://github.com/pAIrprogio/syn-stack.git",
    "directory": "packages/synscript"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test:types": "tsc --noEmit",
    "test:unit": "node --import tsx --test src/**/*.test.ts",
    "test:unit:watch": "node --import tsx --watch --test src/**/*.test.ts",
    "test": "yarn test:types && yarn test:unit",
    "prepare": "yarn test && yarn build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/synscript.index.d.ts",
        "default": "./dist/synscript.index.js"
      },
      "require": {
        "types": "./dist/synscript.index.d.cts",
        "default": "./dist/synscript.index.cjs"
      }
    }
  },
  "dependencies": {
    "@synstack/enhance": "^1.0.1",
    "@synstack/fs": "^1.0.1",
    "@synstack/fs-cache": "^1.0.1",
    "@synstack/git": "^1.0.1",
    "@synstack/glob": "^1.0.1",
    "@synstack/json": "^1.0.1",
    "@synstack/markdown": "^1.0.1",
    "@synstack/path": "^1.0.1",
    "@synstack/pipe": "^1.0.1",
    "@synstack/resolved": "^1.0.1",
    "@synstack/str": "^1.0.1",
    "@synstack/text": "^1.0.1",
    "@synstack/web": "^1.0.1",
    "@synstack/xml": "^1.0.1",
    "@synstack/yaml": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "files": [
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "dist/**/*"
  ],
  "gitHead": "4c97c7cea4508afb05ee10bef0e1d2c3deb71eca"
}
